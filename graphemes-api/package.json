{
  "name": "graphemes-api",
  "version": "0.0.1",
  "description": "API for graphemes register",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "nodemon --watch src --ext ts --exec node --loader ts-node/esm src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "format": "npx prettier . --write",
    "lint": "npx eslint .",
    "test": "npx vitest"
  },
  "dependencies": {
    "any-ascii": "^0.3.2",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "js-levenshtein-esm": "^2.0.0",
    "knex": "^3.1.0",
    "pg": "^8.16.0",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/express": "^5.0.2",
    "@types/node": "^22.15.21",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.27.0",
    "globals": "^16.2.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vitest": "^3.1.4"
  }
}
